{
    "name": "p2media/httpmonitoring",
    "type": "typo3-cms-extension",
    "description": "An extension for the TYP3 CMS which adds a backend module for monitoring the HTTP status code given out by sites.",
    "authors": [
        {
            "name": "Marius Kachel",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.0",
        "typo3/cms-core": "^11.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "P2media\\Httpmonitoring\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "P2media\\Httpmonitoring\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/httpmonitoring": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "httpmonitoring"
        }
    }
}
